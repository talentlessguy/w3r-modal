{
  "name": "w3r-modal",
  "version": "0.0.2",
  "description": "Simple Web3 wallet modal library.",
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./index.css": "./dist/index.css"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "node build.js",
    "postbuild": "tsc --emitDeclarationOnly",
