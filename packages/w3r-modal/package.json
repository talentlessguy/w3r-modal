    "test:coverage": "c8 --include=src pnpm test",
    "test:report": "c8 report --reporter=text-lcov > coverage.lcov",
    "prepublishOnly": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/talentlessguy/w3r-modal.git"
  },
  "keywords": [
    "ethereum",
    "web3",
    "wallet",
    "react",
    "dapp"
  ],
  "author": "v1rtl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/talentlessguy/w3r-modal/issues"
  },
  "homepage": "https://github.com/talentlessguy/w3r-modal#readme",
  "devDependencies": {
    "@ethersproject/providers": "^5.6.5",
    "@testing-library/react": "^13.2.0",
    "@types/jsdom": "^16.2.14",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.0",
    "@vanilla-extract/css": "^1.7.0",
    "@vanilla-extract/esbuild-plugin": "^2.0.3",
    "@web3-react/core": "^8.0.24-beta.0",
    "@web3-react/types": "^8.0.13-beta.0",
    "autoprefixer": "^10.4.5",
    "c8": "^7.11.2",
    "esbuild": "^0.14.38",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.12",
    "react": ">=18",
    "tsm": "^2.2.1",
    "typescript": "^4.6.3",
    "uvu": "^0.5.3"
  },
  "peerDependencies": {
    "@web3-react/core": "^8.0.24-beta.0",
    "@web3-react/types": "^8.0.13-beta.0",
    "react": ">=17"
  },
  "dependencies": {
    "@vanilla-extract/dynamic": ">=2",
    "@vanilla-extract/sprinkles": ">=1.4",
    "focus-visible": "^5.2.0",
    "react-dom": ">=17",
